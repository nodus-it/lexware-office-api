{
    "name": "nodus-it/lexware-office-api",
    "description": "PHP API Client for lexware office",
    "version": "0.1-dev",
    "type": "library",
    "keywords": [
        "lexware",
        "office",
        "api",
        "client"
    ],
    "homepage": "https://github.com/nodus-it/lexware-office-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Bastian Schur",
            "email": "b.schur@nodus-it.de",
            "homepage": "https://www.nodus-it.de",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/nodus-it/lexware-office-api/issues"
    },
    "require": {
        "saloonphp/saloon": "^3.0",
        "saloonphp/rate-limit-plugin": "^2.0.0",
        "spatie/laravel-data": "^4.15.1",
        "saloonphp/pagination-plugin": "^2.2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^v10.2.2",
        "larastan/larastan": "^3.0",
        "laravel/pint": "^1.22",
        "pestphp/pest": "^3.8",
        "fakerphp/faker": "^1.24"
    },
    "autoload": {
        "psr-4": {
            "Nodus\\LexwareOfficeApi\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nodus\\LexwareOfficeApi\\LexwareOfficeApiServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
